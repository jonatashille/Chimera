include macros.asm
pilha SEGMENT PARA STACK 'STACK'
dw  100  dup(? )
pilha ENDS
dados   SEGMENT PARA 'DATA'
dw  10  dup(0)
buffer db 5 dup(' ')
mesg   db  10, ? , 11 dup(0)
dados   ENDS
procs   SEGMENT PARA 'DATA'
dw  32000 dup(0)
procs   ENDS
codigo  SEGMENT PARA 'CODE'
ASSUME SS : pilha, CS : codigo, DS : dados, ES : dados
inicio PROC  FAR
; ----------Inicio das Instrucoes MEPA
INPP
; ---------- Chama Procedimento/Funcao - MAIN
CHPR MAIN,1
DSVS FIM
R3:NADA
; ---------- Entrada Procedimento/Funcao
ENPR 1
; ---------- Alocacao de Memoria (2)
AMEM 2
CRVL 1,-5
ARMZ 1,0
CRVL 1,-7
ARMZ 1,1
CRVL 1,0
CRVI 1,-6
SOMA
CRVL 1,1
SOMA
CRVI 1,-8
SOMA
ARMI 1,-4
; ---------- Desalocacao de Memoria (2)
DMEM 2
RTPR 1,5
; ---------- Fim/Retorno de Procedimento/Funcao
; ----------Inicio MAIN
MAIN:NADA
; ---------- Entrada Procedimento/Funcao
ENPR 1
; ---------- Alocacao de Memoria (3)
AMEM 3
CRCT 0
ARMZ 1,0
CRCT 1
ARMZ 1,1
CRCT 3
ARMZ 1,2
CREN 1,2
CRCT 5
CREN 1,1
CRCT 4
CREN 1,0
; ---------- Chama Procedimento/Funcao - R3
CHPR R3,1
CRVL 1,0
IMPR
IMPE
; ---------- Desalocacao de Memoria (3)
DMEM 3
RTPR 1,0
; ---------- Fim/Retorno de Procedimento/Funcao
FIM:NADA
PARA
; ----------Fim das Instrucoes MEPA
inicio  ENDP
codigo  ENDS
END    inicio
