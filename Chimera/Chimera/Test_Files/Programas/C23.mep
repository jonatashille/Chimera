include macros.asm
pilha SEGMENT PARA STACK 'STACK'
dw  100  dup(? )
pilha ENDS
dados   SEGMENT PARA 'DATA'
dw  10  dup(0)
buffer db 5 dup(' ')
mesg   db  10, ? , 11 dup(0)
dados   ENDS
procs   SEGMENT PARA 'DATA'
dw  32000 dup(0)
procs   ENDS
codigo  SEGMENT PARA 'CODE'
ASSUME SS : pilha, CS : codigo, DS : dados, ES : dados
inicio PROC  FAR
; ----------Inicio das Instrucoes MEPA
INPP
; ---------- Chama Procedimento/Funcao - MAIN
CHPR MAIN,1
DSVS FIM
; ---------- Entrada Rotulo R3
R3:NADA
; ---------- Entrada Procedimento/Funcao
ENPR 1
CRVL 1,0
ARMZ 1,-4
RTPR 1,0
; ---------- Fim/Retorno de Procedimento/Funcao
RTPR 1,31
; ---------- Fim/Retorno de Procedimento/Funcao
; ----------Inicio MAIN
MAIN:NADA
; ---------- Entrada Procedimento/Funcao
ENPR 1
RTPR 1,0
; ---------- Fim/Retorno de Procedimento/Funcao
FIM:NADA
PARA
; ----------Fim das Instrucoes MEPA
inicio  ENDP
codigo  ENDS
END    inicio
