include macros.asm
pilha SEGMENT PARA STACK 'STACK'
dw  100  dup(? )
pilha ENDS
dados   SEGMENT PARA 'DATA'
dw  10  dup(0)
buffer db 5 dup(' ')
mesg   db  10, ? , 11 dup(0)
dados   ENDS
procs   SEGMENT PARA 'DATA'
dw  32000 dup(0)
procs   ENDS
codigo  SEGMENT PARA 'CODE'
ASSUME SS : pilha, CS : codigo, DS : dados, ES : dados
inicio PROC  FAR
; ----------Inicio das Instrucoes MEPA
INPP
; ---------- Chama Procedimento/Funcao - MAIN
CHPR MAIN,1
DSVS FIM
; ---------- Entrada Rotulo R3
R3:NADA
; ---------- Entrada Procedimento/Funcao
ENPR 1
; ---------- Inicio Armazenamento String
CRCT 'R'
ARMI 1,-6
CRCT 'e'
ARMI 1,-5
CRCT 'x'
ARMI 1,-4
; ---------- Fim Armazenamento String
; ---------- Inicio Leitura String
CRVI 1,-6
IMPC
CRVI 1,-5
IMPC
CRVI 1,-4
IMPC
IMPE
RTPR 1,3
; ---------- Fim/Retorno de Procedimento/Funcao
; ----------Inicio MAIN
MAIN:NADA
; ---------- Entrada Procedimento/Funcao
ENPR 1
; ---------- Alocacao de Memoria (3)
AMEM 3
CREN 1,2
CREN 1,1
CREN 1,0
; ---------- Chama Procedimento/Funcao - R3
CHPR R3,1
; ---------- Desalocacao de Memoria (3)
DMEM 3
RTPR 1,0
; ---------- Fim/Retorno de Procedimento/Funcao
FIM:NADA
PARA
; ----------Fim das Instrucoes MEPA
inicio  ENDP
codigo  ENDS
END    inicio
